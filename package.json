{
  "name": "factorio-blueprint-playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "generate:routes": "npx @tanstack/router-cli generate-routefiles",
    "build:types": "npm run generate:routes && tsc --noEmit --project tsconfig.routes.json",
    "lint:fix": "prettier --write \"{src,functions}/**/*.{js,jsx,ts,tsx,css,md,json}\" && eslint . --fix",
    "ci:test": "vitest run",
    "ci:eslint": "eslint . --format json --output-file eslint_report.json",
    "ci:prettier": "prettier --check \"{src,functions}/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "ci:typecheck": "tsc --noEmit",
    "all": "npm run lint:fix && npm run ci:typecheck && npm run build && npm run ci:test"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.72.2",
    "@tanstack/react-query-devtools": "^5.72.2",
    "@tanstack/react-router": "^1.115.2",
    "fflate": "^0.8.2",
    "lucide-react": "^0.488.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250410.0",
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.23.0",
    "@playwright/test": "^1.51.1",
    "@tanstack/react-router-devtools": "^1.115.2",
    "@tanstack/router-cli": "^1.115.2",
    "@tanstack/router-plugin": "^1.115.2",
    "@tanstack/eslint-plugin-router": "^1.114.29",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "@types/rollup-plugin-visualizer": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "sharp": "^0.34.1",
    "tsx": "^4.19.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.6",
    "vitest": "^3.0.2",
    "wrangler": "^4.10.0"
  }
}
