{
  "name": "factorio-blueprint-playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "generate:routes": "npx @tanstack/router-cli generate-routefiles",
    "build:types": "npm run generate:routes && tsc --noEmit --project tsconfig.routes.json",
    "lint:fix": "prettier --write \"{src,functions}/**/*.{js,jsx,ts,tsx,css,md,json}\" && eslint . --fix",
    "ci:eslint": "eslint . --format json --output-file eslint_report.json",
    "ci:prettier": "prettier --check \"{src,functions}/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "ci:typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@preact/signals": "^2.0.1",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "@tanstack/react-router": "^1.102.1",
    "fflate": "^0.8.2",
    "idb-keyval": "^6.2.1",
    "lucide-react": "^0.475.0",
    "preact": "^10.25.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250204.0",
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.18.0",
    "@preact/preset-vite": "^2.10.1",
    "@tanstack/router-cli": "^1.102.1",
    "@tanstack/router-devtools": "^1.102.1",
    "@tanstack/router-plugin": "^1.102.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.1",
    "@types/react-dom": "^19.0.3",
    "@types/rollup-plugin-visualizer": "^4.2.4",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@vitest/coverage-v8": "^3.0.5",
    "eslint": "^9.20.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "sharp": "^0.33.5",
    "tsx": "^4.19.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.24.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.2",
    "wrangler": "^3.107.3"
  }
}
